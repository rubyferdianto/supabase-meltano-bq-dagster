# Example environment variables for Meltano pipeline
# Copy this file to .env and fill in your actual credentials

# Supabase PostgreSQL Credentials
TAP_POSTGRES_HOST='your_supabase_host_here'
TAP_POSTGRES_USER='your_supabase_user_here'
TAP_POSTGRES_PASSWORD='your_supabase_password_here'

# Google Cloud BigQuery Credentials
# Path to the service account JSON key file
GOOGLE_APPLICATION_CREDENTIALS=../service-account-key.json

# BigQuery Configuration
BQ_PROJECT_ID='your-project-id'
TARGET_STAGING_DATASET='olist_data_staging'
TARGET_RAW_DATASET='olist_data_raw'
