# Example environment variables for Meltano pipeline
# Copy this file to .env and fill in your actual credentials

# Supabase PostgreSQL Credentials
TAP_POSTGRES_PASSWORD='your_supabase_password_here'

# Google Cloud BigQuery Credentials
# Store the complete service account JSON as a single environment variable
GOOGLE_CLOUD_CREDENTIALS_JSON='{"type": "service_account", "project_id": "your_project_id", "private_key_id": "your_private_key_id", "private_key": "-----BEGIN PRIVATE KEY-----\nyour_private_key_content_with_newlines\n-----END PRIVATE KEY-----\n", "client_email": "your_service_account_email", "client_id": "your_client_id", "auth_uri": "https://accounts.google.com/o/oauth2/auth", "token_uri": "https://oauth2.googleapis.com/token", "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs", "client_x509_cert_url": "your_client_x509_cert_url", "universe_domain": "googleapis.com"}'

# BigQuery Configuration
TARGET_BIGQUERY_PROJECT='your_bigquery_project_id'
TARGET_BIGQUERY_DATASET='your_dataset_name'
